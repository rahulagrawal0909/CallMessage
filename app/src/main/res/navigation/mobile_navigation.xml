<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.example.rakuten.ui.call.CallFragment"
        android:label="@string/title_call"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/navigation_dashboard"
        android:name="com.example.rakuten.ui.video.YoutubeStreamFragment"
        android:label="@string/title_video"
        tools:layout="@layout/fragment_player" />

    <fragment
        android:id="@+id/navigation_notifications"
        android:name="com.example.rakuten.ui.message.MessageFragment"
        android:label="@string/title_message"
        tools:layout="@layout/fragment_message_two" />
</navigation>